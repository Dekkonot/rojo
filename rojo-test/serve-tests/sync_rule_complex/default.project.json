{
  "name": "sync_rule_complex",
  "tree": {
    "$path": "src"
  },
  "syncRules": [
    {
      "pattern": "*.module",
      "use": "modulescript"
    },
    {
      "pattern": "*.server",
      "use": "serverscript"
    },
    {
      "pattern": "*.client",
      "use": "clientscript"
    },
    {
      "pattern": "*.rojo",
      "use": "project"
    }
  ]
}